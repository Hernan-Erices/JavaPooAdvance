package cuentaDeBanco;

public class BankAccount {

	static String  numeroDeCuenta;
	static double saldoCuentaCorriente;
	static double saldoCuentaAhorro;
	
	//NUMERO ALEATORIO
	public static int obtenerNumeroDiezDigitos() {
	    double fiveDigits = 1000000000 + Math.random() * 999999999;
	    return (int) fiveDigits;
	}
	
	
	
	
	
}
